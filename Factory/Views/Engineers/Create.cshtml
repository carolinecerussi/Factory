
@{
  Layout = "_Layout";
}

@model Factory.Models.Engineer


<h4>Add a new Engineer</h4>

@using (Html.BeginForm())
{
<p>Name: @Html.TextBoxFor(model => model.EName)</p>
<p>Years of Experience: @Html.TextBoxFor(model => model.EExperience)</p>
<p>Assigned Machines: <div class="custom-select" style="width200px;">
  <select>@Html.DropDownList("MId")</select>
  </div>
</p>

  <input type="submit" value="Add new Engineer" />
}

<p>@Html.ActionLink("Show engineers", "Index")</p>